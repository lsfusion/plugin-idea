formEventDeclaration ::= ON
                        (    OK (BEFORE | AFTER)?
                        |    APPLY (BEFORE | AFTER)?
                        |    CLOSE
                        |    INIT
                        |    CANCEL
                        |    DROP
                        |    QUERYOK
                        |    QUERYCLOSE
                        |    changeEventDeclaration
                        |    formContainerEventDeclaration
                        |    scheduleFormEventDeclaration
                        )
                        (REPLACE | NOREPLACE)?
                        formActionPropertyObject
{
    pin = 1
}

changeEventDeclaration ::=
    CHANGE ID
    |
    CHANGE?
        (   OBJECT groupObjectUsage
        |   FILTER groupObjectUsage
        |   ORDER groupObjectUsage
        |   FILTERS groupObjectUsage
        |   ORDERS groupObjectUsage
        |   PROPERTY (BEFORE | AFTER)? propertySelector
        |   FILTERGROUPS filterGroupUsage
        |   FILTERS PROPERTY formPropertyDrawUsage
        )
    ;

formContainerEventDeclaration ::= (COLLAPSE | EXPAND | TAB) (ID | componentSelector)

scheduleFormEventDeclaration ::= SCHEDULE PERIOD intLiteral FIXED?

formFilterGroupDeclaration ::= FILTERGROUP filterGroupName (NULL | NONULL)? regularFilterDeclaration*
{
    pin = 1
    implements = "com.lsfusion.documentation.LSFDocumentation"
