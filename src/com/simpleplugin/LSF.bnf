}

metaDeclId ::=  ID
metaDeclIdList ::= (metaDeclId (COMMA metaDeclId)*)?

metaCodeIdList ::= (metaCodeId (COMMA metaCodeId)*)?

metaCodeId ::= (PRIMITIVE_TYPE | compoundID | metaCodeLiteral)?

metaCodeLiteral ::= LEX_STRING_LITERAL
