eventStatement ::= WHEN baseEvent propertyExpression inlineStatement
		                DO actionPropertyDefinitionBody (ORDER (DESC)? nonEmptyPropertyExpressionList)?
		           (SEMI)? {pin = 1 implements="com.simpleplugin.psi.context.ModifyParamContext" methods = [ getContextModifier getContextInferrer getIcon ] } // Разобраться с точкой с запятой

