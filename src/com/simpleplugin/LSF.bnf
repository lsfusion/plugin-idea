    stubClass = "com.simpleplugin.psi.stubs.MetaStubElement"
    elementTypeFactory = "com.simpleplugin.psi.stubs.types.LSFStubFactory.create"
}

anyTokens ::= (<< readAny >>)*

fakeTwoDashes ::= FAKETWODASHES
fakeThreeDashes ::= FAKETHREEDASHES

private metaStatement ::= statement { recoverWhile = "meta_statement_recover" }
private meta_statement_recover ::= !(statement_start | RBRACE)
metaCodeBody ::= LBRACE (metaStatement*) RBRACE

metaCodeStatement ::= ATSIGN metacodeUsage LBRAC metaCodeIdList RBRAC metaCodeBody? SEMI
{
    pin=2
    mixin="com.simpleplugin.psi.references.impl.LSFMetaReferenceImpl"
