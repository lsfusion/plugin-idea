overrideStatement ::= mappedPropertyClassParamDeclare PLUSEQ
		                (WHEN propertyExpression THEN)?
		                (	propertyExpression
		                |	actionPropertyDefinition
		                )
		                (SEMI)?
{implements="com.simpleplugin.psi.context.ModifyParamContext" methods = [ getContextModifier getContextInferrer getIcon ]}		                    

		                // ;?

